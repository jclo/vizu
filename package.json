{
  "name": "vizu",
  "version": "0.0.9",
  "description": "A Javascript View library for building web and hybrid mobile applications",
  "bin": {},
  "main": "_dist/vizu-min.js",
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov _mocha ./test/main.js --compilers js:babel-core/register",
    "check-coverage": "nyc check-coverage --statements 100 --branches 85 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "report": "nyc report",
    "precommit": "npm run makedist && npm run test && npm run check-coverage && npm run report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/vizu.git"
  },
  "keywords": [
    "ES6",
    "view"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/vizu/issues"
  },
  "homepage": "https://github.com/jclo/vizu",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "4.1.6",
    "babel-preset-env": "1.7.0",
    "babelify": "8.0.0",
    "browserify": "16.2.2",
    "chai": "4.1.2",
    "coveralls": "3.0.1",
    "cross-env": "5.1.6",
    "del": "3.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-header": "2.0.5",
    "gulp-replace": "1.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "husky": "0.14.3",
    "jsdom": "11.11.0",
    "lodash": "4.17.10",
    "mocha": "5.2.0",
    "nyc": "11.8.0",
    "opener": "1.4.3",
    "require-dir": "1.0.0",
    "run-sequence": "2.2.1",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0",
    "watchify": "3.11.0"
  },
  "browser": {},
  "browserify-shim": {},
  "browserify": {},
  "engines": {
    "node": ">=4.0.0"
  }
}
